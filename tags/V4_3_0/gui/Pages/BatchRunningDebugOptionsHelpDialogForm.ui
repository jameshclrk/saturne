<ui version="4.0">
 <class>BatchRunningDebugOptionsHelpDialogForm</class>
 <widget class="QDialog" name="BatchRunningDebugOptionsHelpDialogForm">
  <property name="geometry">
   <rect>
    <x>0</x>
    <y>0</y>
    <width>657</width>
    <height>529</height>
   </rect>
